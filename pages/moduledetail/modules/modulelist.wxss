/* modulelist.wxss */
.md_list_header{
  font-family: serif;
  margin-left: 33rpx;
}
.md_list_header_right{
  float: right;
  margin-right: 30rpx;
}
.md_list_container{
  display: flex;
  flex-direction: column;
  margin: 30rpx;
}
.md_list_item{
  margin: 5rpx;
  border: 2px solid black;
  border-radius: 3px;
  display: flex;
  flex-direction: row;
  height: 250rpx;
}
.md_list_pic{
  height: auto;
  width: 40%;
  margin: 40rpx;
}
.md_list_container_item{
  display: flex;
  flex-direction: row;
  width: 100%;
}
.md_list_container_item_1{
  display: flex;
  flex-direction: column;
}
.md_list_item_price{
  margin-top: 5rpx;
}
.md_list_item_name{
  text-align: center
}
